  "version": "0.8.40",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && cp build/index.html build/404.html",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:e2e": "playwright test --reporter=list",
    "audit": "unlighthouse --site localhost:4173",
    "build:audit": "npm run build && start-server-and-test 'vite preview' http://localhost:4173 'timeout -s SIGKILL 30 unlighthouse --no-cache --disable-gpu --site http://localhost:4173 --ci --build-static || true; node scripts/seo-summary.mjs'",
    "lint": "svelte-check --tsconfig ./tsconfig.json && eslint .",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
